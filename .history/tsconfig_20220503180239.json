{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["./*"]},
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx"
, "components/firebase/Auth.jsx", "components/New_feature/Login/LoginPopup.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
